{
  "compilerOptions": {
    "outDir": "./public/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "types": ["node", "jest", "@testing-library/jest-dom"]
  },
  "include": ["./src/**/*", "./jest-setup.ts"]
}
